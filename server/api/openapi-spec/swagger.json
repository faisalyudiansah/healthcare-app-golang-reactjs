    "/auth/login": {
      "post": {
        "description": "Login with registered account",
        "tags": ["Auth"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLoginOrRegister"
              },
              "examples": {
                "Success": {
                  "value": {
                    "email": "example@gmail.com",
                    "password": "Example123@"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "success login",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "access_token": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "Success Login": {
                    "value": {
                      "message": "success",
                      "data": {
                        "access_token": "token"
                      }
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "account is not verified",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                },
                "examples": {
                  "Unverified": {
                    "value": {
                      "message": "your account is not verified"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
